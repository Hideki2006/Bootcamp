#include <stdio.h>
int main(){
    int valor;
    int alcool = 0, gasolina = 0, diesel = 0;

    while(1){

        scanf("%d", &valor);
        if(valor == 4){
            break;
        }

        switch (valor){
        case 1:
            alcool++;
            break;
        case 2:
            gasolina++;
            break;
        case 3:
            diesel++;
            break;
        
        default:
            break;
        }
        
    }

    printf("MUITO OBRIGADO\n");
    printf("alcool %d\n", alcool);
    printf("gasolina %d\n", gasolina);
    printf("diesel %d\n", diesel);

    return 0;
}
